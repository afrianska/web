---
import StudyCase from "../../layouts/MarkdownStudyCaseLayout.astro";

import { getCollection, render } from "astro:content";

export async function getStaticPaths() {
  const studyCases = await getCollection("studyCases");
  return studyCases.map((studyCase) => ({
    params: { id: studyCase.id },
    props: { studyCase },
  }));
}
const { studyCase } = Astro.props;
---

<StudyCase studyCase={studyCase} />
